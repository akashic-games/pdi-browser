{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "ES2021"],
    "types": [],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strict": true,
    "alwaysStrict": true,
    "declaration": true,
    "useUnknownInCatchVariables": false,
    "module": "commonjs",
    "outDir": "./lib/full"
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "./src/**/__tests__/**/*"
  ]
}
